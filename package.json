{
  "name": "direct-upload",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "backend",
    "frontend",
    "types"
  ],
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "npm run build --workspace=backend",
    "build:frontend": "npm run build --workspace=frontend",
    "build:types": "npm run build --workspace=types",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "npm run test --workspace=backend",
    "test:frontend": "npm run test --workspace=frontend",
    "test:types": "npm run test --workspace=types",
    "publish": "npm run publish:types && npm run publish:backend && npm run publish:frontend",
    "publish:backend": "npm run publish --workspace=backend",
    "publish:frontend": "npm run publish --workspace=frontend",
    "publish:types": "npm run publish --workspace=types"
  },
  "devDependencies": {
    "typescript": "^4.5.5"
  }
}
