  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^16.18.114",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.0.0",
    "typescript": "^4.5.5"
